
public class StringPlus {

	public static void main(String[] args) {
		int v1 = 10;
		int v2 = 20;
		// int v1 = 10, v2 = 20;
		
		int res = v1 + v2;	
		System.out.println(res);
		
		String str1 = "abc";
		String str2 = "def";
		String sres = str1 + str2;
		System.out.println(sres);
		
		System.out.println(str1 + res); // abc30
		
		String seven = "7";
		char seven2 = '7';
		int seven3 = 7;
		System.out.println(seven + seven2 + seven3);
	}

}
