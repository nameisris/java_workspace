
public class OperTest {

	public static void main(String[] args) {
		int i = -10;
		i = +i; // i * 1
		System.out.println(i); // -10
		
		i = -1; // i*(-1)
		System.out.println(i); // 10
		
		boolean power = false;
		power = !power; // false => true 변경
		System.out.println(power); // true
		power = !power;
		System.out.println(power); // true

		int j = 10;
		System.out.println(j);
		j = ~j;
		System.out.println(j);
	}

}
