
public class MyInfo {

	public static void main(String[] args) {
		// 이름을 저장할 변수 선언하여 본인 이름 할당
		String name = "류인수";
		
		// 나이를 저장할 변수 선언하여 본인 나이 할당
		int age = 27;
		
		// 키를 저장할 변수 선언하여 본인 키 소수점까지 할당
		float height = 173.5f;
		
		// 남여 구분을 저장할 변수를 선언하여 본인 성별 할당 'F' or 'M'
		char gender = 'M';

	}

}
